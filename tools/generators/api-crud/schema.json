{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "api-crud",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Project name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "model": {
      "type": "string",
      "description": "Name of the model"
    }
  },
  "required": [
    "name",
    "model"
  ]
}